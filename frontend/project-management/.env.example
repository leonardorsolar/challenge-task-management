# .env.example
# Copie este arquivo para .env e ajuste as URLs conforme necessário

# URL do servidor Node.js (para Vite use VITE_, para Create React App use REACT_APP_)
VITE_NODE_SERVER_URL=http://localhost:3001

# URL do servidor Python
VITE_PYTHON_SERVER_URL=http://localhost:8000

# Outras configurações opcionais
VITE_CHECK_INTERVAL=30000

# Se você estiver usando Create React App ao invés de Vite, use estas variáveis:
# REACT_APP_NODE_SERVER_URL=http://localhost:3001
# REACT_APP_PYTHON_SERVER_URL=http://localhost:8000